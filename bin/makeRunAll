#!/bin/bash

EXE=./bin/exe
IN_FILE=inputfiles/subwaydata_newstationnames.csv
nl='
'

touch temp.txt

#create a temp file for storing commands

#loop files in commandfiles
for file in commandfiles/*; do
    cat $file >> temp.txt
  done

echo $nl
echo "running all commands...$nl"

#execute the program
echo "reading inputfile... $nl"
$EXE $IN_FILE temp.txt

rm temp.txt
